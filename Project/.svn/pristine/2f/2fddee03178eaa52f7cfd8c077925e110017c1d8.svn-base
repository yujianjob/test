        <script type="text/javascript">
        	document.addEventListener("deviceready",myDeviceReady,false);
        	function myDeviceReady(){
        		document.addEventListener('backbutton', myBackButton, false);
        	}
        	function myBackButton(){
        		cordova.exec(function(str){ }, function(err) { alert(err) }, "CommonFun", "toBack", ['1']);
        	}
        	
		var Phone = function(){
			
		}
		Phone.prototype.call = function(successCallback,failureCallback,number){
			cordova.exec(successCallback,failureCallback,"Phone","call",[number]);
		}
		window.Phone = new Phone();
		var dadianhua = function(orderid,phoneNumber,operatorid){
			window.Phone.call(function(obj){
				var str = JSON.stringify(obj);
				var thjson = eval('(' + str + ')');
				var starttime = thjson.startTime;//通话开始时间
				var second = thjson.duration;

				var ts=getTs();
				var str=actid+orderid+starttime+second+operatorid+version+ts;
				str=str.toUpperCase();
				var hash=hex_md5(str);
				hash=hash.toUpperCase();
				hash=hash+Key;
				hash=hex_md5(hash);
				hash=hash.toUpperCase();
				var vkey=hash;

				var postData = {"actid":actid,"orderid":orderid,"starttime":starttime,"second":second,"operatorid":operatorid,"version":version,"ts":ts,"vkey": vkey };
				//alert(starttime+"--"+second+"--"+postData);
				//return;
				$.ajax({
					type: "post",
					url: host+"/CallUserDetail",
					data: postData,
					dataType: "json",
					success: function(data){
					//alert(333333);
						if(data.code==0){
							
						}else{
							showPopup(data.message);
						}
					},
					error:function(msg){
						showPopup(msg);	
					}
				 });
			},function(error){
				alert(JSON.stringify(obj));
			},phoneNumber);		
		}
        </script>
