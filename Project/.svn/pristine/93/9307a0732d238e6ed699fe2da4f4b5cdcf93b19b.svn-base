package com.landaojia.washclothes.userappserver.service.order;

import java.math.BigDecimal;
import java.util.List;

import com.landaojia.washclothes.userappserver.dao.orderorder.OrderSimpleVo;

/**
 * 订单详情，返回值
 * @author liuxi
 */
public class OrderDetailRo {
	
	/**
	 * 订单简要信息
	 */
	private OrderSimpleVo orderSimpleVo;
	
	/**
	 * 洗涤条目
	 */
	private List<WashItemSubtVo> washItemSubtList;
	
	/**
	 * 总金额
	 */
	private BigDecimal totalPrice;

	public OrderSimpleVo getOrderSimpleVo() {
		return orderSimpleVo;
	}

	public void setOrderSimpleVo(OrderSimpleVo orderSimpleVo) {
		this.orderSimpleVo = orderSimpleVo;
	}

	public List<WashItemSubtVo> getWashItemSubtList() {
		return washItemSubtList;
	}

	public void setWashItemSubtList(List<WashItemSubtVo> washItemSubtList) {
		this.washItemSubtList = washItemSubtList;
	}

	public BigDecimal getTotalPrice() {
		return totalPrice;
	}

	public void setTotalPrice(BigDecimal totalPrice) {
		this.totalPrice = totalPrice;
	}

}
