/*/////////////////////////查看订单样式///////////////////////*/
.myorder{
	margin:3%; padding-bottom:11%;
}
/*订单详情*/
	.order-detail h2,.payment-info h2,.address-info h2,.goods-detail h2,.order-remarks h2{
		font-size:16px;
		height:40px;
		line-height:40px;
		font-family:"微软雅黑";
		font-weight:normal;
		border-bottom:1px solid #ebebeb;
	}
.order-detail{
	 padding:0 3%;
	 background:white;
	 -webkit-box-shadow:0px 0px 4px rgba(0,0,0,0.5);
	 -moz-box-shadow:0px 0px 4px rgba(0,0,0,0.5);
	 box-shadow:0px 0px 4px rgba(0,0,0,0.5);
}
	.order-detail .order-table{
		width:100%;
	}
		.order-detail .order-table td{
			padding:10px;
			font-family:"微软雅黑";
		}
			.order-detail .payment-btn{
				float:right;
				width:65%; 
				height:auto;
			}
/*支付信息*/
.payment-info{
	 padding:0 3%;
	 background:white;
	 -webkit-box-shadow:0px 0px 4px rgba(0,0,0,0.5);
	 -moz-box-shadow:0px 0px 4px rgba(0,0,0,0.5);
	 box-shadow:0px 0px 4px rgba(0,0,0,0.5);
	 margin-top:3%;
}
	.payment-info .payment-tabs{
		width:100%;
	}
		.payment-info .payment-tabs td{
			padding:5px 10px;
			font-family:"微软雅黑";
		}
			.payment-info .payment-tabs .totel-price{
				font-size:18px;
				font-weight:bold;
			}
/*地址信息*/
.address-info{
	 padding:0 3%;
	 background:white;
	 -webkit-box-shadow:0px 0px 4px rgba(0,0,0,0.5);
