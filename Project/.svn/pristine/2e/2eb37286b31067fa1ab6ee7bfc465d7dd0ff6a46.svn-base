/*首页样式*/
.index-banner{width:100%;padding:0;	position:relative;	/* On older OS versions "position" and "z-index" must be defined, */z-index:1;			/* it seems that recent webkit is less picky and works anyway. */
	overflow:hidden;}
		.carousel{width:100%;margin:0 auto; position:relative;}
			  .banner{height:auto !important;}
					 .banner li{display:none;}
							.banner li img{ width:100%; height:auto !important;}
					 .banner .firstImg{display:block;}
			 .digital{ position:absolute;left:8%;bottom:5%;}
					 .digital li{float:left;margin:0 3px;}
								.digital li a{width:12px;height:11px;line-height:11px; text-decoration:none;text-align:center;color:#fff;background:url(../images/icon2.png) no-repeat;display:block;}
								.digital li a:hover{background:#CF4500; text-decoration:none;}
					  .cur a{width:12px;height:11px; background:url(../images/icon1.png) no-repeat !important;line-height:20px;text-align:center;}

.indexList{padding:4% 4% 11% 4%; clear:both}
		.indexList ul{padding:0;margin:0;}
		.indexList ul li{ float:left; width:49%; height:auto !important; padding-bottom:1%;}
		.indexList ul li img{ width:100%; height:auto !important;}
.hdbanner{margin: 0px auto; padding: 0px; width: 100%; background-color: rgb(21, 21, 21); position:relative}
#wrapper {margin: 0px; width: 100%; height: auto; overflow: hidden; position: relative; z-index: 1;}
#scroller {
	padding: 0px; height: auto; float: left;
}
#scroller ul {
	list-style: none; margin: 0px; padding: 0px; width: 100%; height: auto; text-align: left; float: left; display: block;
}
#scroller li {
	padding: 0px; width: auto; height: auto; text-align: center; font-size: 0px; float: left; display: block; position: relative;
}
#scroller li a {
	margin: 0px; padding: 0px; display: block;
}
#scroller li p {
	margin: 0px; padding: 0px; width: 100%; text-align: left; bottom: 0px; color: rgb(244, 244, 244); line-height: 24px; text-indent: 10px; font-size: 14px; display: block; white-space: nowrap; position: absolute; z-index: 2; text-overflow: ellipsis; background-color: rgba(0, 0, 0, 0.5);
}
#nav {
	padding: 0px; width: auto; margin-top: -15px; float: right; position: relative; z-index: 3;
}
#indicator > li {
	list-style: none; margin: 0px; padding: 0px; float: left; display: block;
}
#indicator {
	margin: 0px 8px; padding: 0px; width: auto; display: block;
}
#indicator > li {
	background: rgb(255, 255, 255); border-radius: 4px; width: 8px; height: 8px; text-indent: -9999em; overflow: hidden; margin-right: 4px; -webkit-border-radius: 4px; -moz-border-radius: 4px; -o-border-radius: 4px; -webkit-box-shadow:2px 2px 4px rgba(0,0,0,0.5) inset;-moz-box-shadow:2px 2px 4px rgba(0,0,0,0.5) inset;box-shadow:2px 2px 4px rgba(0,0,0,0.5) inset;
}
#indicator > li.active {
	background:green;
}
#indicator > li:last-child {
	margin: 0px;
}
.prev{ position:absolute; left:0px; top:0px;width:30px; height:50px; background:rgba(0,0,0,0.5); z-index:20;color:white;font-size:25px;font-weight:bold; text-align:center; line-height:50px;}
.next{ position:absolute; right:0px; top:0px;width:30px; height:50px; background:rgba(0,0,0,0.5); z-index:20;color:white;font-size:25px;font-weight:bold; text-align:center; line-height:50px}
