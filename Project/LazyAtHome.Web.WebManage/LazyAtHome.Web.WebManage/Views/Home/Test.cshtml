                                    $.post(url, data, DWZ.ajaxDone, "json");
                                }
                            });
                        }
                    </script>
                    <a class="button" href="javascript:;" onclick="testConfirmMsg('demo/common/ajaxDone.html')"><span>确认（是/否）</span></a><br /><br />
